  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnTigue/EbolaMapper.git"
  },
  "devDependencies": {
    "browserify": "^8.1.0",
    "browserify-shim": "^3.8.2",
    "bunyan-debug-stream": "^1.0.5",
    "chai": "*",
    "chai-as-promised": "*",
    "jshint": "^2.6.3",
    "mocha": "*",
    "must": "^0.12.0",
    "nock": "^1.6.1",
    "sinon": "*"
  },
  "scripts": {
    "env": "env",
    "lint": "jshint test/*.js --exclude node_modules",
    "test": "tape test/test-name-to-loc-cache-builder.js|node_modules/bunyan/bin/bunyan"
