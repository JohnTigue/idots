    "leaflet-markercluster": {
      "depends": [
        "leaflet"
      ]
    }
  },
  "dependencies": {
    "bluebird": "^2.9.24",
    "bunyan": "^1.3.4",
    "superagent-bluebird-promise": "^2.0.2",
    "utilios": "git://github.com/JohnTigue/utilios"
  }
}
