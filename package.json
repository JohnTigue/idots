{
  "name": "ebola-mapper",
  "version": "0.1.0",
  "license": "Apache 2.0",
  "devDependencies": {
    "browserify": "^8.1.0",
    "browserify-shim": "^3.8.2",
    "promise": "^6.1.0",
    "tape": "^3.5.0"
  },
  "scripts": {
    "env": "env",
    "lint": "jshint test/test-name-to-loc-cache-builder.js --exclude node_modules",
